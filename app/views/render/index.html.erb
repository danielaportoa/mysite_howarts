<h1>Personajes de Harry Potter</h1>
<br>
<p>Ejemplo Personajes, Locacion, House, Magia </p>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Location</th>
            <th scope="col">House</th>
            <th scope="col">Spell</th>
        </tr>
    </thead>
    <tbody>
        <% @characters.each do |c| %>
            <tr>
                <th scope="row">
                    <%= c.id %>
                </th>
                    <td>
                        <%= c.name %>
                    </td>
                    <td>
                        <%= c.location %>
                    </td>
                    <td>
                        <%= c.house%>
                    </td>
                    <td>
                        <%= c.spell%>
                    </td>
            </tr>
        <%end %>
    </tbody>
</table>